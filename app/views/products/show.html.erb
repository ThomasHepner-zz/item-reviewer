<h1><%= @product.name %></h1>
<h2><%= @product.description %></h2>
<div>
  <button id="new-form"><span>+ </span>Add review</button>
</div>
<div>
  <% @reviews.each do |review| %>
    <%= render partial: 'reviews/show', locals: { review: review } %>
  <% end %>
</div>
<%= render partial: 'reviews/new' %>
